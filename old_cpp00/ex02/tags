!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.8	//
ALLC	Makefile	/^ALLC = $(DSRC)$/;"	m
Account	inc/Account.hpp	/^class Account {$/;"	c
CC	Makefile	/^CC = c++$/;"	m
CFLAGS	Makefile	/^CFLAGS = -Wall -Wextra -Werror -Iinc$/;"	m
DSRC	Makefile	/^DSRC = $(addprefix $(SRDIR), $(SRCS))$/;"	m
NAME	Makefile	/^NAME = account$/;"	m
OBJS	Makefile	/^OBJS = $(DSRC:.cpp=.o)$/;"	m
RM	Makefile	/^RM = rm -rf$/;"	m
SRCS	Makefile	/^SRCS =  tests.cpp$/;"	m
SRDIR	Makefile	/^SRDIR = src\/$/;"	m
__ACCOUNT_H__	inc/Account.hpp	/^#define __ACCOUNT_H__$/;"	d
_accountIndex	inc/Account.hpp	/^	int				_accountIndex;$/;"	m	class:Account
_amount	inc/Account.hpp	/^	int				_amount;$/;"	m	class:Account
_nbAccounts	inc/Account.hpp	/^	static int	_nbAccounts;$/;"	m	class:Account
_nbDeposits	inc/Account.hpp	/^	int				_nbDeposits;$/;"	m	class:Account
_nbWithdrawals	inc/Account.hpp	/^	int				_nbWithdrawals;$/;"	m	class:Account
_totalAmount	inc/Account.hpp	/^	static int	_totalAmount;$/;"	m	class:Account
_totalNbDeposits	inc/Account.hpp	/^	static int	_totalNbDeposits;$/;"	m	class:Account
_totalNbWithdrawals	inc/Account.hpp	/^	static int	_totalNbWithdrawals;$/;"	m	class:Account
main	src/account.cpp	/^int	main(int arg, char **args)$/;"	f
main	src/tests.cpp	/^int		main( void ) {$/;"	f
t	inc/Account.hpp	/^	typedef Account		t;$/;"	t	class:Account
